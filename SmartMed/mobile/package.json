{
  "name": "smartmed-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:smoke": "jest --testPathPattern=smoke --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SmartMed.xcworkspace -scheme SmartMed -configuration Release -destination generic/platform=iOS -archivePath SmartMed.xcarchive archive",
    "clean": "react-native clean-project-auto",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/auth": "^18.6.1",
    "@react-native-firebase/firestore": "^18.6.1",
    "@react-native-firebase/storage": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "@react-native-firebase/crashlytics": "^18.6.1",
    "@react-native-firebase/analytics": "^18.6.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@reduxjs/toolkit": "^1.9.7",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-camera": "^4.2.1",
    "react-native-document-picker": "^9.1.1",
    "react-native-elements": "^3.4.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-image-picker": "^7.0.3",
    "react-native-permissions": "^3.10.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "^13.14.0",
    "react-native-text-recognition": "^1.0.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-vision-camera": "^3.6.9",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "react-native-uuid": "^2.0.1",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react-native-device-info": "^10.11.0",
    "react-native-share": "^10.0.2",
    "react-native-pdf": "^6.7.0",
    "react-native-fs": "^2.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.37",
    "@types/react-test-renderer": "^18.0.5",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.76.8",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/jest-native": "^5.4.3",
    "firebase-functions-test": "^3.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
